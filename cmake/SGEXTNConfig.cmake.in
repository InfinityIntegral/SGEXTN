@PACKAGE_INIT@
if(NOT DEFINED SGEXTN_INSTALL_FOLDER)
    include("${CMAKE_CURRENT_LIST_DIR}/buildlah.cmake")
    set(SGEXTN_INSTALL_FOLDER "${CMAKE_CURRENT_LIST_DIR}")
    message(STATUS "found SGEXTN at ${SGEXTN_INSTALL_FOLDER}")
    message(STATUS "this does NOT provide SGEXTN targets, only BuildLah CMake integration is provided, to get SGEXTN targets pls run BuildLah_find_dependencies() macro")
else()
    include("${SGEXTN_INSTALL_FOLDER}/SGEXTN_install_targets.cmake")
    message(STATUS "SGEXTN targets have been provided")
endif()
